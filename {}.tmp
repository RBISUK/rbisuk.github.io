<script>
(async()=>{
  const set=(id,v)=>{const el=document.getElementById(id); if(el) el.textContent=v;}
  try {
    const v=await fetch('/version.json').then(r=>r.json());
    set('m-deployed', v.deployed || '—');
  } catch(e){ set('m-deployed','—'); }
  try {
    const s=await fetch('/trust/status/current.json').then(r=>r.json());
    set('m-status', (s.status||'—').toUpperCase());
  } catch(e){ set('m-status','—'); }
})();
</script>
