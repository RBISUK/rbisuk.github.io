<main id="main">
  <div class="toolbar">
    <input id="q" type="search" placeholder="Search products, tags…">
    <select id="cat">
      <option value="">All categories</option>
    </select>
    <select id="status">
      <option value="">All statuses</option>
      <option>Live</option><option>Beta</option><option>Planned</option>
    </select>
  </div>
  <div id="grid" class="grid" aria-live="polite"></div>
</main>
<footer>
  Data source: <code>/rbis.products.json</code>. Links open sections pages.
